.bytecode 56.0
.source "table_switch.jasm"
.class public final dk/skrypalle/jasm/it/disassembler/TableSwitch
.super java/lang/Object

# prints 'zero\none\ntwo\n' to stdout
.method public static main([Ljava/lang/String;)V
  ldc -1
  invokestatic dk/skrypalle/jasm/it/disassembler/TableSwitch.doSwitch:(I)V
  ldc 0
  invokestatic dk/skrypalle/jasm/it/disassembler/TableSwitch.doSwitch:(I)V
  ldc 1
  invokestatic dk/skrypalle/jasm/it/disassembler/TableSwitch.doSwitch:(I)V
  ldc 2
  invokestatic dk/skrypalle/jasm/it/disassembler/TableSwitch.doSwitch:(I)V
  ldc 100
  invokestatic dk/skrypalle/jasm/it/disassembler/TableSwitch.doSwitch:(I)V
  ldc 200
  invokestatic dk/skrypalle/jasm/it/disassembler/TableSwitch.doSwitch:(I)V
  return
.end method

.method private static doSwitch(I)V
  iload 0
  tableswitch
          0: label_0
          1: label_1
          2: label_2
    default: label_3
  endswitch
label_0:
  getstatic java/lang/System.out:Ljava/io/PrintStream;
  ldc "zero"
  invokevirtual java/io/PrintStream.println:(Ljava/lang/String;)V
  goto label_3
label_1:
  getstatic java/lang/System.out:Ljava/io/PrintStream;
  ldc "one"
  invokevirtual java/io/PrintStream.println:(Ljava/lang/String;)V
  goto label_3
label_2:
  getstatic java/lang/System.out:Ljava/io/PrintStream;
  ldc "two"
  invokevirtual java/io/PrintStream.println:(Ljava/lang/String;)V
  goto label_3
label_3:
  return
.end method
