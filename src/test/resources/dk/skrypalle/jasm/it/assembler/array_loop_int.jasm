  newarray I
  dup
  ldc 0
  ldc 4
  iastore
  dup
  ldc 1
  ldc 3
  iastore
  dup
  ldc 2
  ldc 2
  iastore
  dup
  ldc 3
  ldc 1
  iastore
  dup
  ldc 4
  ldc 0
  iastore
  astore 1
  aload 1
  astore 2
  aload 2
  arraylength
  istore 3
  ldc 0
  istore 4
label_0:
  iload 4
  iload 3
  if_icmpge label_1
  aload 2
  iload 4
  iaload
  istore 5
  getstatic java/lang/System.out:Ljava/io/PrintStream;
  iload 5
  invokevirtual java/io/PrintStream.println:(I)V
  iinc 4 1
  goto label_0
